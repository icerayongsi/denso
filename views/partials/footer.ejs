<script>
    
        const refreshTime = () => {
            const dateString = new Date();

            let addzeroHour = "";
            let addzeroMinute = "";
            let addzeroSecond = "";
            if (dateString.getHours().toString().length == 1) addzeroHour = "0";
            if (dateString.getMinutes().toString().length == 1) addzeroMinute = "0";
            if (dateString.getSeconds().toString().length == 1) addzeroSecond = "0";

            let ampm = dateString.getHours( ) >= 12 ? ' PM' : ' AM';
            let date = dateString.getMonth() + 1 + "/" + dateString.getDate() + "/" + dateString.getFullYear(); 

            let time = addzeroHour + dateString.getHours() + ":" + addzeroMinute +  dateString.getMinutes() + ":" + addzeroSecond + dateString.getSeconds() + " : " + ampm;
            $("#date").html(date);
            $("#time").html(time);
        }

        setInterval(refreshTime, 100);

</script>
